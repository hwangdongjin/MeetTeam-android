<?xml version="1.0" encoding="utf-8"?>
<com.example.inyoung.teamapp.CheckaleLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="35dp"
    android:background="#00000000"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="27dp"
        android:layout_gravity="center_vertical|center_horizontal"
        app:cardBackgroundColor="#ffffff"
        app:cardCornerRadius="10dp"
        app:cardElevation="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:gravity="center_horizontal|center_vertical">

            <CheckBox
                android:id="@+id/manager_checkbox"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:buttonTint="#000000"
                android:gravity="center_vertical|center_horizontal|start" />

            <TextView
                android:id="@+id/manager_do11"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:textColor="#000000"
                android:textSize="12dp" />


            <TextView
                android:id="@+id/manager_Name11"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="manager"
                android:layout_marginLeft="10dp"
                android:textColor="#000000"
                android:textSize="12dp" />
        </LinearLayout>
    </android.support.v7.widget.CardView>


</com.example.inyoung.teamapp.CheckaleLinearLayout>